    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@unocss/reset": "^0.53.5",
    "@urql/core": "^4.0.11",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "graphql": "^16.7.1",
    "lunr": "^2.3.9",
    "prettier": "^2.8.0",
    "prettier-plugin-css-order": "^1.3.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "typescript": "^5.0.0",
    "unplugin-icons": "^0.16.5",
